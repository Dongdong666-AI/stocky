# Stocky MCP Server Environment Variables
# Copy this file to .env and add your API keys

# API Keys for stock image providers
# Pexels API Key
# Get your free API key at: https://www.pexels.com/api/
PEXELS_API_KEY=your_pexels_api_key_here

# Unsplash Access Key
# Get your free access key at: https://unsplash.com/developers
UNSPLASH_ACCESS_KEY=your_unsplash_access_key_here

# Feature flags
# Set to 'true' to include attribution links in image results
ENABLE_ATTRIBUTION_LINKS=false
